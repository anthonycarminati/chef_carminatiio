[program:carminatiio]
command = gunicorn manage:app
directory = /opt/carminatiio
user = root
environment = MAIL_SERVER="<%= MAIL_SERVER %>", MAIL_PORT=<%= MAIL_PORT %>, MAIL_USERNAME="<%= MAIL_USERNAME %>", MAIL_PASSWORD="<%= MAIL_PASSWORD %>", MAIL_USE_SSL=False, MAIL_USE_TLS=True, SQLALCHEMY_DATABASE_URI="<%= SQLALCHEMY_DATABASE_URI %>", FLASK_CONFIG="<%= FLASK_CONFIG %>"