[program:carminatiio]
command = gunicorn manage:app
directory = /opt/carminatiio
user = root
environment = MAIL_SERVER=<%= node[:app_config][:env_var][:MAIL_SERVER] %>, MAIL_PORT=<%= MAIL_PORT %>, MAIL_USERNAME=<%= MAIL_USERNAME %>, MAIL_PASSWORD=<%= MAIL_PASSWORD %>, MAIL_USE_SSL=<%= MAIL_USE_SSL %>, MAIL_USE_TLS=<%= MAIL_USE_TLS %>, SQLALCHEMY_DATABASE_URI=<%= SQLALCHEMY_DATABASE_URI %>, FLASK_CONFIG=<%= FLASK_CONFIG %>
